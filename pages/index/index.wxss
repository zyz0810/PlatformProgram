/* 商品列表 */
@import '/template/productList/productListTemp.wxss';

page {
  position: relative;
  background: #fff;
}

.bg {
  position: fixed;
  top: -340rpx;
  left: 0;
  width: 100%;
  height: 700rpx;
  overflow-x: hidden;
}

.bg_cont {
  position: absolute;
  top: 0;
  left: -25vw;
  width: 150vw;
  height: 700rpx;
  background: #db1516;
  background-image: linear-gradient(to right, #db1516, #fe5e60);
  border-radius: 50%;
}

.scrollWrap {
  display: block;
  height: 100%;
  width: 100%;
  padding-top: 44px;
  position: relative;
  z-index: 9;
}

.scrollWrap swiper-item {
  position: absolute;
  width: 100%;
  height: 100%;
  transform: translate(0%, 0px) translateZ(0px);
}

.search {
  top: 0;
  left: 0;
  width: 100%;
  box-sizing: border-box;
  z-index: 110;
  padding: 15rpx 0;
  /* background: #fff; */
  height: 88rpx;
}

.search.borderbottom::after {
  content: "";
  display: block;
  width: 100%;
  position: absolute;
  bottom: -3rpx;
  right: 0;
  height: 1px;
  background: #fff;
  transform: scaleY(0.5);
  z-index: 2;
}

.searchWrap {
  width: 700rpx;
  background: #f5f5f5;
  margin: 0 auto;
  box-sizing: border-box;
  padding: 10rpx 100rpx;
  border-radius: 30rpx;
  position: relative;
  color: #fff;
  /* border: 1px solid #ef2b2d; */
}

.searchWrap image {
  display: block;
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 10rpx;
}

.searchWrap input {
  height: 40rpx;
  min-height: 40rpx;
  color: #000;
}

.searchWrap .searchIcon {
  left: 20rpx;
}

.swiperWrap {
  position: relative;
  /* background: #fff; *//* margin-top: 1000rpx; */
}

.swiperWrap image {
  border-radius: 20rpx;
  box-shadow: 0px 15px 10px -15px #ccc;
}

.swiperWrap .newsWrap {
  width: 95%;
  margin: 0 auto;
  background: #fff;
  position: absolute;
  bottom: -50rpx;
  left: 2.5%;
  border-radius: 10rpx;
  padding: 25rpx 0;
}

.wrapperContainer {
  width: 100%;
}

.adSwiper image {
  width: 96%;
}

.topInfo {
  /* width: 750rpx; */
  margin: 0 2%;
  padding: 20rpx 50rpx;
  background: #fff;
  /* border-bottom: 1rpx solid #ececec; */
}

.topInfo .flex-item {
  display: flex;
  align-items: center;
  justify-content: center;
}

.topInfo .flex-item text {
  color: rgb(73, 73, 73);
  display: inline-block;
}

.topInfo .flex-item image {
  width: 28rpx;
  height: 28rpx;
  padding-right: 10rpx;
}

