/* 拼团抢购 */

.collage {
  padding: 40rpx 4%;
  background: #fff;
}

.collage_tag {
  color: #ffe3e3;
  background: #fd407a;
  display: inline-block;
  font-style: italic;
  padding: 2px 10px;
  border-top-left-radius: 16px;
  border-bottom-right-radius: 16px;
  margin-left: 10px;
}

.collage .collage_list {
  padding-top: 4%;
}

.collage .collage_list .lineItem {
  padding: 0;
  margin-bottom: 20rpx;
  border: 1px solid #edeceb;
}

.collage .collage_list .lineItem .left {
