  height: 265rpx;
}

.collage .collage_list .lineItem .left image {
  width: 100%;
  height: 100%;
}

.collage .collage_list .lineItem .center {
  justify-content: space-between;
  flex-direction: column;
  position: relative;
  display: flex;
  flex-wrap: nowrap;
  margin: 10rpx;
}

.collage .collage_list .lineItem .oriPrice {
  text-decoration: line-through;
}

.collage .collage_list .lineItem .collage_count {
  background: #fd407a;
