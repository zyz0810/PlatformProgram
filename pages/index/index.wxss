  padding: 8rpx 18rpx;
  border-radius: 6rpx;
}

.collage .collage_list .lineItem .goCollage {
  color: #fd407a;
}

/* 限时抢购 */

.timeLimit {
  padding: 20rpx 2%;
  margin: 0;
  background: #fff;
  border-bottom: 10rpx solid #f4f4f4;
}

.timeLimit_tag {
  color: #ffe3e3;
  background: #fd407a;
  display: inline-block;
  font-style: italic;
  padding: 2px 10px;
  border-top-left-radius: 16px;
  border-bottom-right-radius: 16px;
  margin-left: 10px;
}

.timeLimit .timeLimit_list {
  /* padding-top: 4%; *//* background: #fceac4; */
}

.timeLimit .timeLimit_list .lineItem {
  -webkit-box-direction: normal;
  -webkit-box-orient: horizontal;
  -webkit-flex-direction: row;
  -moz-flex-direction: row;
  flex-direction: row;
  -webkit-box-align: stretch;
  -webkit-align-items: stretch;
  -moz-align-items: stretch;
  align-items: stretch;
  /* margin-bottom: 10rpx; */
  padding: 20rpx;
  border-radius: 20rpx;
}

.timeLimit .timeLimit_list .lineItem:last-child {
  margin-bottom: 0;
}

.timeLimit .timeLimit_list .lineItem .center {
  padding: 0 0 0 2%;
  -webkit-box-direction: normal;
  -webkit-box-orient: vertical;
  -webkit-flex-direction: column;
  -moz-flex-direction: column;
  flex-direction: column;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  -moz-justify-content: space-between;
  justify-content: space-between;
}

.timeLimit .timeLimit_list .lineItem .left {
  width: 200rpx;
  height: 200rpx;
}

.timeLimit .timeLimit_list .lineItem .left image {
  border-radius: 8px;
  width: 200rpx;
  height: 200rpx;
}

.timeLimit_list .lineItem .center .tag {
  line-height: 1;
}

.timeLimit_list .lineItem .center .tag text {
  color: #ff9c9c;
  background: #ffeded;
  border-radius: 3px;
  line-height: 1.5;
  padding: 0 5px;
  margin-right: 5px;
}

/* .timeLimit_list .lineItem .center .price text{
  margin-right: 20px;
} */

.timeLimit_list .lineItem .center .price {
  position: relative;
  flex-direction: column;
}

