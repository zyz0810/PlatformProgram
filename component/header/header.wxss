.header {
  width: 100%;
}

.header-placeholder {
  width: 100%;
  height: 128rpx;
}

.header-wrap {
  top: 0;
  display: flex;
  flex-direction: row;
  /* align-items: center; */
  width: 100%;
  height: 44px;
  padding-top: 20px;
}
.teannt_name{
  text-align: center;
  width: 350rpx;
  margin: 0 auto;
}

.header-wrap .back{
  position: relative;
  width: 24px;
  height: 24px;
  padding: 0;
  margin: 10px 0 0 16px;
  color: inherit;
  background-color: transparent;
}
.header-wrap .back text::before{
  position: absolute;
  top: 3px;
  left: 0;
  /* font-family: "iconfont"; */
  font-size: 20px;
  line-height: 1;
  content: "\e67c";
  /* border: 0; */
}

.header-wrap .back::before {
  /* position: absolute; */
  /* top: 3px; */
  /* left: 0; */
  /* font-family: "iconfont"; */
  /* font-size: 20px; */
  /* line-height: 1; */
  /* content: "\e67c"; */
  /* border: 0; */
  display: none;
}

.header-wrap .back::after {
  display: none;
}

.header-title {
  flex: 1;
  padding: 0 16px;
  font-size: 17px;
  text-align: center;
  font-weight: bold;
  position: relative;
  line-height: 44px;
  border: 0;
}

.header-right {
  width: 56px;
}

.header.isx .header-wrap {
  padding-top: 44px;
}

.header.isx .header-placeholder {
  height: 88px;
}

.header.android .header-placeholder {
  height: 72px;
}

.header.android .header-wrap {
  /* height: 76rpx; */
  height: 84rpx;
  padding-top: 24px;
}

.header.android .header-wrap .back::before {
  content: "\e734";
}

.header.android .header-title {
  text-align: center;
}
.header{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}
.nav_btn{
  position: absolute;
  left: 0;
  top: 8px;
  z-index: 99;
  padding: 0 10px;
  width: 50rpx;
}
.nav_btn image{
  width: 50rpx;
  height: 50rpx;
}
.f20{
  font-size: 40rpx;
}
.nav_platform{
  font-size: 28rpx;
  position: absolute;
  top: 44px;
  left: 0rpx;
  z-index: 99999;
  background: #fff;
  line-height: 2;
  padding: 0 20rpx;
  box-shadow: 0 3px 5px rgb(179, 171, 171);
  border-radius: 10rpx;
}
.nav_platform text{
  margin-right: 10rpx;
}
.tl{
  text-align: left;
}

.clr_mediumGray {
  color: #434343;
}
/* 超出一行显示省略号 */

.overone {
  white-space: nowrap;
  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  overflow: hidden;
}